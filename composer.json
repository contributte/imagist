{
	"name": "contributte/imagist",
	"type": "library",
	"license": ["MIT"],
	"require": {
		"php": ">=7.4",
		"nette/utils": "^3.2",
		"league/flysystem": "^1.1",
		"psr/event-dispatcher": "^1.0"
	},
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.3",
        "phpstan/phpstan": "^0.12.11",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^0.12.2",
        "ninjify/qa": "^0.10.0",
        "codeception/module-phpbrowser": "^1.0.0",
		"ramsey/uuid": "^4.1",
		"doctrine/dbal": "^2.12",
		"symfony/serializer": "^5.2",
		"doctrine/annotations": "^1.10",
		"superbalist/flysystem-google-storage": "^7.2",
		"symfony/filesystem": "^5.2",
		"tracy/tracy": "^2.8",
		"nette/http": "^3.1",
		"latte/latte": "^2.9",
		"nette/forms": "^3.1",
		"nette/application": "^3.1",
		"nette/di": "^3.0",
		"imagine/imagine": "^1.2",
		"spatie/temporary-directory": "^1.3",
		"symfony/http-foundation": "^5.2",
		"symfony/dependency-injection": "^5.2",
		"symfony/http-kernel": "^5.2"
    },
    "autoload": {
		"psr-4": {
			"Contributte\\Imagist\\": "src/"
		}
	},
    "autoload-dev": {
        "psr-4": {
            "Contributte\\Imagist\\Testing\\": "tests/src",
            "Contributte\\Imagist\\Testing\\Unit\\": "tests/unit",
            "Contributte\\Imagist\\Testing\\Functional\\": "tests/functional"
        }
    }
}
